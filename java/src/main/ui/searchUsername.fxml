<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="349.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.SearchUsernameController">
   <children>
      <Label alignment="CENTER" layoutX="30.0" layoutY="29.0" prefHeight="39.0" prefWidth="428.0" text="Edit Employee">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="85.0" layoutY="148.0" prefHeight="58.0" prefWidth="318.0" promptText="Username">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Label fx:id="errorLabel" layoutX="85.0" layoutY="124.0" prefHeight="24.0" prefWidth="278.0" text="Enter Username To Search">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button layoutX="202.0" layoutY="248.0" mnemonicParsing="false" onAction="#search" text="Search">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="30.0" layoutY="308.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
