<AnchorPane prefHeight="349.0" prefWidth="488.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.controller.ForgottenPassword">
   <children>
      <Label fx:id="resetTitle" alignment="CENTER" layoutX="13.0" layoutY="14.0" prefHeight="67.0" prefWidth="464.0" text="Password Reset For&#10;$username" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="secretQuestion" alignment="CENTER" layoutX="57.0" layoutY="95.0" prefHeight="35.0" prefWidth="374.0" text="\$secretQuestion">
